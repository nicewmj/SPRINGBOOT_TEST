<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.SupermanMapper">


    <select id="select"  parameterType="com.example.entity.Superman" resultType="com.example.entity.Superman">
        select  * from t_superman where pid = #{pid}
    </select>

    <select id="selectAll"  resultType="com.example.entity.Superman">
        select  * from t_superman
    </select>


</mapper>